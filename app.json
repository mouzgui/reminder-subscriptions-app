{
  "expo": {
    "name": "Zombie Subscriptions",
    "slug": "zombie-subscriptions",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "zombie-subs",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#32a6ff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zombiesubs.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#32a6ff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.zombiesubs.app"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "01946639-dfa5-4364-be26-926d691eaf9f"
      }
    }
  }
}
